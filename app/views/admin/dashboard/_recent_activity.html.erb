<h4>Recent Activity</h4>
<table class="table" border="0" cellpadding="0" cellspacing="0">
	<thead>
		<tr>
			<th>Kind</th>
			<th>Title</th>
			<th>Edited By</th>
			<th>Updated At</th>
			<th>Created At</th>
		</tr>
	</thead>
	<tfoot>
		<tr>
			<td colspan="5">
				<%= will_paginate @recently_active_objects %>
			</td>
		</tr>
	</tfoot>
	<tbody>
		<%= render :partial => 'activity_object', :collection => @recently_active_objects %>
	</tbody>
</table>