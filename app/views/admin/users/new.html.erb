<% content_for :header do %>
	<%= render :partial => 'admin/shared/main_menu' %>
	<%= error_messages_for :user %>
	<div id="sub_nav">
		<ul>
			<li>
				<%= link_to 'Show All Users', admin_users_path %>
			</li>
			<li>
				<%= link_to 'Create User', new_admin_user_path %>
			</li>
		</ul>
	</div>
	<div id="search_menu">
		<%= render :partial => 'admin/shared/search/users/non_ajax' %>
	</div>
<% end %>
<div id="sidebar">
	<%= render :partial => "sidebar" %>
</div>
<div id="content">
	<h3><span class="cat_type">Create A New</span> User</h3>
	<% form_for [:admin, @user], :html => { :multipart => true, :method => :put } do |f| %>
		<%= render :partial => "form", :object => f %>
	<% end %>
</div>