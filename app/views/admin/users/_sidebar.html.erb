<h3>Browse Users</h3>
<ul id="sidebar_nav" class="standard">
<%- for user in @users -%>
	<li <%- if user == @user -%>class="active"<%- end -%>>
		<%= link_to sanitize(h(user.name)), admin_user_path(user) %>
		<ul class="sidebar_actions">
			<li>
				<%= link_to image_tag("/images/icons/shape_square_edit.png"), edit_admin_user_path(user), :title => "Edit this user." %>
			</li>
			<li>
				<%= link_to image_tag("/images/icons/delete.png"), admin_user_path(user), :confirm => 'Are you sure?', :method => :delete, :title => "Delete this user" %>
			</li>
		</ul>
	</li>
<%- end -%>
</ul>
<div class="pagination black">
	<%= will_paginate @users, :param_name => :users_page %>
</div>